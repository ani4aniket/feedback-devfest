<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta
      name="viewport"
      content="width=device-width, initial-scale=1, shrink-to-fit=no"
    />
    <meta http-equiv="x-ua-compatible" content="ie=edge" />
    <title>DevFest' 19 Kolkata Feedback Form</title>
    <!-- Font Awesome -->
    <link
      rel="stylesheet"
      href="https://use.fontawesome.com/releases/v5.8.2/css/all.css"
    />
    <!-- Bootstrap core CSS -->
    <link href="css/bootstrap.min.css" rel="stylesheet" />
    <!-- Material Design Bootstrap -->
    <link href="css/mdb.min.css" rel="stylesheet" />
    <!-- Your custom styles (optional) -->
    <link href="css/style.min.css" rel="stylesheet" />
    <style>
      .search-form {
        border-radius: 30px 0px 0px 30px;
        /*border-radius:Top-left, Top-right, Bottom-right, Bottom-left;*/
      }
      /*to cange only one form and not all give unique class name like  class="search-form" */
      .search-btn {
        border-radius: 0px 30px 30px 0px;
        cursor: pointer;
      }
    </style>
  </head>

  <body>
    <!--Main Navigation-->
    <header>
      <!-- Navbar -->
      <nav
        class="navbar fixed-top navbar-expand-lg navbar-light white scrolling-navbar"
      >
        <div class="container">
          <!-- Brand -->
          <a
            class="navbar-brand waves-effect"
            href=""
            target="_blank"
            style="display:flex; justify-content:center; align-items:center "
          >
            <img
              src="https://pbs.twimg.com/media/D22N_huX4AEbb1y.jpg"
              style="height: 50px"
              alt=""
              srcset=""
            />
            <strong class="grey-text">GDG Kolkata</strong>
          </a>

          <!-- Collapse -->
          <button
            class="navbar-toggler"
            type="button"
            data-toggle="collapse"
            data-target="#navbarSupportedContent"
            aria-controls="navbarSupportedContent"
            aria-expanded="false"
            aria-label="Toggle navigation"
          >
            <span class="navbar-toggler-icon"></span>
          </button>

          <!-- Links -->
          <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <!-- Left -->
            <ul class="navbar-nav mr-auto"></ul>

            <!-- Right -->
            <ul class="navbar-nav nav-flex-icons">
              <li class="nav-item">
                <a
                  href="https://www.facebook.com/mdbootstrap"
                  class="nav-link waves-effect"
                  target="_blank"
                >
                  <i class="fab fa-facebook-f"></i>
                </a>
              </li>
              <li class="nav-item">
                <a
                  href="https://twitter.com/MDBootstrap"
                  class="nav-link waves-effect"
                  target="_blank"
                >
                  <i class="fab fa-twitter"></i>
                </a>
              </li>
              <li class="nav-item">
                <a
                  href="https://github.com/mdbootstrap/bootstrap-material-design"
                  class="nav-link rounded waves-effect"
                  target="_blank"
                >
                  <i class="fa fa-globe"></i>
                </a>
              </li>
            </ul>
          </div>
        </div>
      </nav>
      <!-- Navbar -->
    </header>
    <!--Main Navigation-->

    <!--Main layout-->
    <main class="mt-5 pt-5">
      <div class="container">
        <!--Section: Jumbotron-->
        <section
          class="card wow fadeIn"
          style="background-image: url(https://mdbootstrap.com/img/Photos/Others/gradient1.jpg);"
        >
          <!-- Content -->
          <div class="card-body text-white text-center py-5 px-5 my-5">
            <h1 class="mb-4">
              <strong>DevFest Kolkata 2019</strong>
            </h1>
            <h2 class="mb-3">
              <strong>Feedback </strong>
              <br />
            </h2>
            <p>Coupons Inside!</p>
            <br />
            <br />
            <div
              style="display: flex; justify-content: center; align-items: flex-start"
              class="py-3"
            >
              <form
                id="search-form"
                class="form-inline"
                role="form"
                method="post"
                action="/feedback"
              >
                <div class="input-group">
                  <input
                    type="email"
                    class="form-control search-form my-2"
                    placeholder="Email Address"
                    name="email"
                    required
                  />

                  <span class="input-group-btn submit-button"
                    ><button type="submit" class="btn btn-secondary search-btn">
                      <i class="fa fa-arrow-right"></i></button
                  ></span>
                </div>
              </form>
            </div>
          </div>

          <!-- Content -->
        </section>
        <!--Section: Jumbotron-->

        <!--Section: Cards-->
      </div>
    </main>
    <!--Main layout-->

    <!--/.Footer-->

    <!-- SCRIPTS -->
    <!-- JQuery -->
    <script type="text/javascript" src="js/jquery-3.4.1.min.js"></script>
    <!-- Bootstrap tooltips -->
    <script type="text/javascript" src="js/popper.min.js"></script>
    <!-- Bootstrap core JavaScript -->
    <script type="text/javascript" src="js/bootstrap.min.js"></script>
    <!-- MDB core JavaScript -->
    <script type="text/javascript" src="js/mdb.min.js"></script>
    <!-- Initializations -->
    <script type="text/javascript">
      // Animations initialization
      new WOW().init();
    </script>
  </body>
</html>
